{
  "name": "marionette-responsive-avatar",
  "version": "0.1.1",
  "description": "Marionette component to render avatar or initials if no avatar provided",
  "main": "build/marionette-responsive-avatar.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "gulp build/dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avasuro/marionette-responsive-avatar.git"
  },
  "keywords": [
    "backbone",
    "marionette",
    "avatar",
    "plugin",
    "responsive"
  ],
  "author": "Alex Vasiuro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/avasuro/marionette-responsive-avatar/issues"
  },
  "homepage": "https://github.com/avasuro/marionette-responsive-avatar#readme",
  "github": "https://github.com/avasuro/marionette-responsive-avatar",
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "ie >= 8"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "syntax-class-properties",
      "syntax-object-rest-spread",
      [
        "transform-class-properties",
        {
          "spec": false
        }
      ],
      "transform-object-rest-spread"
    ]
  },
  "peerDependencies": {
    "backbone": "~1.3",
    "backbone.marionette": "~3.0.0",
    "jquery": "~1.12.0",
    "underscore": "~1.8.0"
  },
  "devDependencies": {
    "avasuro-eslint-config": "git+https://github.com/avasuro/eslint-config.git",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "backbone": "^1.3.3",
    "backbone.marionette": "^3.4.3",
    "eslint": "^4.6.1",
    "eslint-plugin-import": "^2.7.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-group": "^1.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "jquery": "^1.12.4",
    "underscore": "^1.8.3",
    "underscore-template-loader": "^0.8.0"
  }
}
